{
    "comment": "ActiveTrader ",
    "components": {
        "Order Book": {
            "window": {
                "url": "$applicationRoot/components/chartiq/chartiq/orderBook.html",
                "top": "center",
                "left": "center",
                "width": 800,
                "height": 602,
                "addToWorkspace": true,
                "options": {
                    "securityPolicy": "trusted"
                },
                "affinity": "charts"
            },
            "component": {
                "mode": "example",
                "inject": false,
                "spawnOnStartup": false,
                "category": "system",
                "preload": [
                    "$applicationRoot/components/chartiq/active-trader/active-trader-orderbook.js"
                ]
            },
            "foreign": {
                "services": {
                    "windowService": {
                        "canGroup": true,
                        "manageWindowMovement": true,
                        "allowAutoArrange": true,
                        "allowTabbing": true,
                        "allowTiling": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "showLinker": true,
                        "FSBLHeader": {
                            "component": "windowTitleBar",
                            "bumpElements": {
                                "fixed": "0Positioned",
                                "absolute": "0Positioned",
                                "bumpBy": "0px"
                            },
                            "bodyMarginTop": "0px",
                            "forceHeaderHeight": false
                        },
                        "persistWindowState": true,
                        "title": "Order Book",
                        "hackScrollbar": false
                    },
                    "Toolbar": {
                        "iconURL": "$applicationRoot/assets/img/IQ-icon.png"
                    }
                }
            }
        },
