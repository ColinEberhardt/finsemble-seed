{
    "comment": "Symphony Integration Component",
    "$schema": "../../../../core/configs/schemas/fileBasedSchemas/appdConfigFile.schema.json",
    "appd": {
        "Company": {
            "name": "ShareToSymphony",
            "appId": "ShareToSymphony",
            "description": "Share message to symphony",
            "manifest": {
                "window": {
                    "url": "http://localhost:3000/",
                    "width": 300,
                    "height": 400,
                    "options": {
                        "minWidth": 75,
                        "securityPolicy": "trusted"
                    }
                },
                "foreign": {
                    "components": {
                        "App Launcher": {
                            "launchableByUser": true
                        },
                        "Toolbar": {
                            "iconClass": "ff-chart-advanced"
                        },
                        "Window Manager": {
                            "FSBLHeader": false,
                            "persistWindowState": true
                        }
                    }
                },
                "component": {
                    "preload": []
                }
            },
            "version": "1.0.0",
            "tooltip": "Share To Symphony",
            "images": [
                {
                    "url": "https://i.imgur.com/10C3LdH.png",
                    "tooltip": "Share To Symphony"
                }
            ],
            "tags": ["symphony", "fdc3"],
            "contactEmail": "info@cosaic.io",
            "supportEmail": "support@finsemble.com",
            "publisher": "Cosaic",
            "icons": [
                {
                    "url": "https://i.imgur.com/mpBdiHd.png"
                }
            ],
            "intents": [
                {
                    "name": "ShareToSymphony",
                    "displayName": "Share To Symphony",
                    "contexts": ["fdc3.symphonyContent"]
                }
            ]
        }
    }
}
